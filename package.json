{
  "name": "@dinazor/core",
  "version": "1.1.36",
  "description": "@dinazor/core is a Angular2+ component core library.",
  "main": "release/index.js",
  "typings": "release/index.d.ts",
  "scripts": {
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:release": "rimraf release",
    "clean:build": "rimraf build",
    "start": "webpack-dev-server",
    "start:hmr": "webpack-dev-server --env.HMR",
    "build": "npm run build:webpack",
    "build:webpack": "webpack --display-error-details",
    "build:package": "cross-env NODE_ENV=package npm run build",
    "build:css": "postcss --use autoprefixer dist/*.css -d dist/",
    "build:sass": "node ./config/sass.js",
    "copy:build": "cpx -v \"src/**/*\" \"build\"",
    "copy:styles": "cpx -v \"build/**/*.css\" \"release\"",
    "release": "cross-env NODE_ENV=production npm run build",
    "version": "chg release -y && git add -A CHANGELOG.md",
    "package": "npm-run-all -s clean:release copy:build build:sass package:replace-scss package:aot build:package copy:styles package:minify package:minifycss clean:build",
    "package:ts": "tsc --outDir release",
    "package:aot": "ngc -p tsconfig-aot.json",
    "package:minify": "uglifyjs release/index.js --source-map --compress --mangle --screw-ie8 --output release/index.min.js",
    "package:minifycss": "uglifycss --max-line-len 500 --expand-vars release/index.css > release/index.min.css",
    "package:replace-scss": "node ./config/replace.js",
    "deploy": "node ./config/deploy.js",
    "tag": "release-it"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "author": "Atakan Atik",
  "license": "MIT",
  "keywords": [
    "angular2",
    "angularjs",
    "angular4",
    "angular",
    "components",
    "directives"
  ],
  "repository": {
    "type": "git",
    "url": "git+http://git.klcyazilim.com/dinazor-client-libs/dn-core.git"
  },
  "homepage": "http://git.klcyazilim.com/dinazor-client-libs/dn-core",
  "bugs": {
    "url": "http://git.klcyazilim.com/dinazor-client-libs/dn-core/issues"
  },
